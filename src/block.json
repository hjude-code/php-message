{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "hjude/php-message",
	"version": "0.1.0",
	"title": "Php-message",
	"category": "text",
	"icon": "smiley",
	"description": "Allows dynamic message to be generated on live site",
	"supports": {
		"html": false,
		"color": {},
		"typography": {
			"fontSize": true
		},
		"spacing": {
			"padding":true,
			"margin":true
		},
		"defaultStylePicker": true
	},
	"attributes": {
		"copy":{
			"type":"string",
			"default":"message here"
		}
	},
	"textdomain": "php-message",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php"
}
